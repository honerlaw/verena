    "@onerlaw/verena-server": "*",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~6.20.0",
    "@tamagui/animations-react-native": "^1.132.23",
    "@tamagui/babel-plugin": "^1.132.23",
    "@tamagui/config": "^1.132.23",
    "@tamagui/core": "^1.132.23",
    "@tamagui/font-inter": "^1.132.23",
    "@tamagui/lucide-icons": "^1.132.23",
    "@tamagui/metro-plugin": "^1.132.23",
    "@tamagui/theme-base": "^1.132.23",
    "@tamagui/toast": "^1.132.23",
    "@tanstack/react-query": "^5.87.4",
    "@trpc/client": "^11.5.1",
    "@trpc/tanstack-react-query": "^11.5.1",
    "base-64": "^1.0.0",
