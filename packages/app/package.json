{
  "name": "@onerlaw/verena-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --noEmit",
    "clean": "rm -rf node_modules .vscode package-lock.json ios android .expo-env.d.ts .tamagui .expo dist",
    "clean:cache": "rm -rf .expo .tamagui ios android .expo-env.d.ts dist $TMPDIR/react-* && watchman watch-del-all",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:prod": "NODE_ENV=production npm run ios",
    "web": "expo start --web",
    "web:export": "npm run clean:cache && expo export --clear -p web",
    "lint": "expo lint",
    "lint:format": "expo lint --fix",
    "submit:ios": "eas workflow:run submit-ios.yml",
    "submit:android": "eas workflow:run submit-android.yml"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.30",
    "@clerk/clerk-react": "^5.46.2",
    "@expo/vector-icons": "^15.0.2",
